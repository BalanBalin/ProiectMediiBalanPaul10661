@page
@model ProiectMediiBalanPaul10661.Pages.Programari.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Programari</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Programare[0].Pacient)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Programare[0].Doctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Programare[0].Serviciu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Programare[0].DataProgramare)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Programare) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Pacient)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Serviciu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataProgramare)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
